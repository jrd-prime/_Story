using UnityEngine;

namespace _StoryGame.Data.Main
{
    [CreateAssetMenu(
        fileName = nameof(HeroASettings),
        menuName = SOPathConst.MainSettings + nameof(HeroASettings),
        order = 100)]
    public class HeroASettings : ASettingsBase
    {
        [field: SerializeField] public float Speed { get; private set; } = 5f;
        [field: SerializeField] public float RotationSpeed { get; private set; } = 5f;
    }
}
